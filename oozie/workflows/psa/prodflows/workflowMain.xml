<workflow-app xmlns="uri:oozie:workflow:0.2" name="sqoop-wf">
    <start to="forkingMain"/>
	<fork name="forkingMain">
        <path start="sqoop-subFlow1"/>
        <path start="sqoop-subFlow2"/>
		<path start="sqoop-subFlow3"/>
        <path start="sqoop-subFlow4"/>
        <path start="sqoop-subFlow5"/>
        <path start="sqoop-subFlow6"/>
        <path start="sqoop-subFlow7"/>
        <path start="sqoop-subFlow8"/>
    </fork>
    <action name="sqoop-subFlow1">
        <sub-workflow>
            <app-path>${nameNode}/user/oozie/share/workflows/psa/prodflows/workflow_1.xml</app-path>
            <propagate-configuration/>
        </sub-workflow>
        <ok to="joiningMain"/>
		<error to="joiningMain"/>
    </action>
    <action name="sqoop-subFlow2">
        <sub-workflow>
            <app-path>${nameNode}/user/oozie/share/workflows/psa/prodflows/workflow_2.xml</app-path>
            <propagate-configuration/>
        </sub-workflow>
        <ok to="joiningMain"/>
		<error to="joiningMain"/>
    </action>
    <action name="sqoop-subFlow3">
        <sub-workflow>
            <app-path>${nameNode}/user/oozie/share/workflows/psa/prodflows/workflow_3.xml</app-path>
            <propagate-configuration/>
        </sub-workflow>
        <ok to="joiningMain"/>
		<error to="joiningMain"/>
    </action>
    <action name="sqoop-subFlow4">
        <sub-workflow>
            <app-path>${nameNode}/user/oozie/share/workflows/psa/prodflows/workflow_4.xml</app-path>
            <propagate-configuration/>
        </sub-workflow>
        <ok to="joiningMain"/>
		<error to="joiningMain"/>
    </action>
    <action name="sqoop-subFlow5">
        <sub-workflow>
            <app-path>${nameNode}/user/oozie/share/workflows/psa/prodflows/workflow_5.xml</app-path>
            <propagate-configuration/>
        </sub-workflow>
        <ok to="joiningMain"/>
		<error to="joiningMain"/>
    </action>
    <action name="sqoop-subFlow6">
        <sub-workflow>
            <app-path>${nameNode}/user/oozie/share/workflows/psa/prodflows/workflow_6.xml</app-path>
            <propagate-configuration/>
        </sub-workflow>
        <ok to="joiningMain"/>
		<error to="joiningMain"/>
    </action>
    <action name="sqoop-subFlow7">
        <sub-workflow>
            <app-path>${nameNode}/user/oozie/share/workflows/psa/prodflows/workflow_7.xml</app-path>
            <propagate-configuration/>
        </sub-workflow>
        <ok to="joiningMain"/>
		<error to="joiningMain"/>
    </action>
    <action name="sqoop-subFlow8">
        <sub-workflow>
            <app-path>${nameNode}/user/oozie/share/workflows/psa/prodflows/workflow_8.xml</app-path>
            <propagate-configuration/>
        </sub-workflow>
        <ok to="joiningMain"/>
		<error to="joiningMain"/>
    </action>
    <join name="joiningMain" to="endMain"/>
    <kill name="failMain">
		<message>Sqoop failed, error message[${wf:errorMessage(wf:lastErrorNode())}]</message>
	</kill>
	<end name="endMain"/>
</workflow-app>
